/**********页签项**********/
QTabWidget::pane {
        border: none;
        background: rgb(236, 237, 238);
}
QTabWidget::tab-bar {
        border: none;
}
QFrame#m_RibbornBar {
        border: none;
        background: rgb(43, 87, 154);
        height: 10px;
}
QTabBar#m_centralTab::tab,
QTabBar#m_rightAttrTab::tab {
        border: none;
        color: black;
        background: rgb(238,244,254);
        height: 30px;
        min-width: 80px;
        padding-left: 7px;
        padding-right: 7px;
}
QTabBar#m_centralTab::tab:hover,
QTabBar#m_rightAttrTab::tab:hover {
        background: rgb(247,249,255);
        color:rgb(43, 87, 154);
}
QTabBar#m_centralTab::tab::selected,
QTabBar#m_rightAttrTab::tab:selected {
        color:rgb(43, 87, 154);
        background: rgb(255, 255, 255);
}

QTabBar::tab {
        border: none;
        color: black;
        background: rgb(238,244,254);
        height: 22px;
        min-width: 100px;
        padding-left: 7px;
        padding-right: 7px;
}
QTabBar::tab:hover {
        background: rgb(247,249,255);
        color:rgb(43, 87, 154);
}
QTabBar::tab::selected {
        color:rgb(43, 87, 154);
        background: rgb(255, 255, 255);
}

